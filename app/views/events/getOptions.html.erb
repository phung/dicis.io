<div class="main">

<div class="logo_event">
	<h1>dicis.io</h1>
</div>

<h2 class="option_title">What is your option?</h2>

<table>
  <thead>
    <tr>
      <th><h2 class="option_title">Name</h2></th>
    </tr>
  </thead>

  <tbody>
    <% @options.each do |option| %>
      <tr>
        <td><%= option.name %></td>
      </tr>
    <% end %>
  </tbody>

<%= render 'formoption' %>
</table>

</div>
